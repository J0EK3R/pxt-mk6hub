{
    "name": "mk6hub",
    "version": "0.0.1",
    "description": "MK6 Hub",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "enums.d.ts",
        "shims.d.ts",
        "linebeacon.ts",
        "linebeacon.cpp",
        "LineBeaconService.cpp",
        "LineBeaconService.h",
        "MKBLEUtils.cpp",
        "MKBLEUtils.h",
        "CryptTool.cpp",
        "CryptTool.h"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "7.0.51",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "pairing_mode": 0,
                    "whitelist": 0,
                    "security_level": null
                }
            }
        },
        "optionalConfig": {
            "microbit-dal": {
                "stack_size": 1280,
                "gatt_table_size": "0x700"
            }
        }
    }
}
